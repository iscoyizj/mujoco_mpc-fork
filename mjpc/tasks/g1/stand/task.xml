<mujoco model="G1 Stand">
  <include file="../../common.xml" />
  <include file="../g1.xml" />
  <size memory="400K" />

  <custom>
    <numeric name="agent_planner" data="2" />
    <numeric name="agent_horizon" data="0.40" />
    <numeric name="agent_timestep" data="0.02" />
    <numeric name="sampling_spline_points" data="3" />
    <numeric name="sampling_exploration" data="0.05" />
    <numeric name="gradient_spline_points" data="5" />
    <numeric name="residual_Height Goal" data="1.2 0.5 1.2" />
    <numeric name="residual_Foot Goal" data="0.3 0.0 0.5" />
  </custom>

  <sensor>
    <!-- user="[norm_type] [weight] [weight_lower_bound] [weight_upper_bound] [norm_parameters...]" -->
    <user name="Height" dim="1" user="6 50.0 0.0 100.0 0.1" />
    <user name="Balance" dim="1" user="6 50.0 0.0 100.0 0.1" />
    <user name="CoM Vel." dim="2" user="0 3.0 0.0 10.0" />
    <user name="Joint Vel." dim="21" user="0 0.5 0.0 1.0" />
    <user name="Control" dim="21" user="3 0.03 0.0 0.3 1.0" />
    <user name="Posture" dim="21" user="0 0.05 0.0 0.1 0.3" />
    <user name="Upright" dim="12" user="6 10.0 0 100 3.0" />
    <user name="Foot Relative Position" dim="4" user="6 1.0 0.0 10.0 1.0" />
    <framepos name="trace0" objtype="body" objname="torso" />
    <framepos name="torso_position" objtype="body" objname="torso" />
    <framepos name="head_position" objtype="body" objname="head" />
    <framequat name="head_orientation" objtype="body" objname="head" />
    <framezaxis name="torso_zaxis" objtype="body" objname="torso" />
    <subtreelinvel name="pelvis_subtreelinvel" body="pelvis" />
    <subtreecom name="pelvis_subtreecom" body="pelvis" />
    <framexaxis name="pelvis_forward" objtype="body" objname="pelvis" />
    <framepos name="sp0" objtype="site" objname="sp0" />
    <framepos name="sp1" objtype="site" objname="sp1" />
    <framepos name="sp2" objtype="site" objname="sp2" />
    <framepos name="sp3" objtype="site" objname="sp3" />

    <framepos name="right_foot" objtype="site" objname="foot_right" />
    <framepos name="left_foot" objtype="site" objname="foot_left" />

    <framepos name="tracking_pos[pelvis]" objtype="site" objname="tracking[pelvis]" />
    <framepos name="tracking_pos[head]" objtype="site" objname="tracking[head]" />
    <framepos name="tracking_pos[ltoe]" objtype="site" objname="tracking[ltoe]" />
    <framepos name="tracking_pos[rtoe]" objtype="site" objname="tracking[rtoe]" />
    <framepos name="tracking_pos[lheel]" objtype="site" objname="tracking[lheel]" />
    <framepos name="tracking_pos[rheel]" objtype="site" objname="tracking[rheel]" />
    <framepos name="tracking_pos[lknee]" objtype="site" objname="tracking[lknee]" />
    <framepos name="tracking_pos[rknee]" objtype="site" objname="tracking[rknee]" />
    <framepos name="tracking_pos[lhand]" objtype="site" objname="tracking[lhand]" />
    <framepos name="tracking_pos[rhand]" objtype="site" objname="tracking[rhand]" />
    <framepos name="tracking_pos[lelbow]" objtype="site" objname="tracking[lelbow]" />
    <framepos name="tracking_pos[relbow]" objtype="site" objname="tracking[relbow]" />
    <framepos name="tracking_pos[lshoulder]" objtype="site" objname="tracking[lshoulder]" />
    <framepos name="tracking_pos[rshoulder]" objtype="site" objname="tracking[rshoulder]" />
    <framepos name="tracking_pos[lhip]" objtype="site" objname="tracking[lhip]" />
    <framepos name="tracking_pos[rhip]" objtype="site" objname="tracking[rhip]" />

    <framelinvel name="tracking_linvel[pelvis]" objtype="site" objname="tracking[pelvis]" />
    <framelinvel name="tracking_linvel[head]" objtype="site" objname="tracking[head]" />
    <framelinvel name="tracking_linvel[ltoe]" objtype="site" objname="tracking[ltoe]" />
    <framelinvel name="tracking_linvel[rtoe]" objtype="site" objname="tracking[rtoe]" />
    <framelinvel name="tracking_linvel[lheel]" objtype="site" objname="tracking[lheel]" />
    <framelinvel name="tracking_linvel[rheel]" objtype="site" objname="tracking[rheel]" />
    <framelinvel name="tracking_linvel[lknee]" objtype="site" objname="tracking[lknee]" />
    <framelinvel name="tracking_linvel[rknee]" objtype="site" objname="tracking[rknee]" />
    <framelinvel name="tracking_linvel[lhand]" objtype="site" objname="tracking[lhand]" />
    <framelinvel name="tracking_linvel[rhand]" objtype="site" objname="tracking[rhand]" />
    <framelinvel name="tracking_linvel[lelbow]" objtype="site" objname="tracking[lelbow]" />
    <framelinvel name="tracking_linvel[relbow]" objtype="site" objname="tracking[relbow]" />
    <framelinvel name="tracking_linvel[lshoulder]" objtype="site" objname="tracking[lshoulder]" />
    <framelinvel name="tracking_linvel[rshoulder]" objtype="site" objname="tracking[rshoulder]" />
    <framelinvel name="tracking_linvel[lhip]" objtype="site" objname="tracking[lhip]" />
    <framelinvel name="tracking_linvel[rhip]" objtype="site" objname="tracking[rhip]" />
  </sensor>

  <actuator>
    <!-- <motor name="left_hip_pitch_joint" joint="left_hip_pitch_joint" gear="80" />
        <motor name="left_hip_roll_joint" joint="left_hip_roll_joint" gear="80" />
        <motor name="left_hip_yaw_joint" joint="left_hip_yaw_joint" gear="80" />
        <motor name="left_knee_joint" joint="left_knee_joint" gear="100" />
        <motor name="left_ankle_pitch_joint" joint="left_ankle_pitch_joint" gear="50" />
        <motor name="left_ankle_roll_joint" joint="left_ankle_roll_joint" gear="50" />

        <motor name="right_hip_pitch_joint" joint="right_hip_pitch_joint" gear="80" />
        <motor name="right_hip_roll_joint" joint="right_hip_roll_joint" gear="80" />
        <motor name="right_hip_yaw_joint" joint="right_hip_yaw_joint" gear="80" />
        <motor name="right_knee_joint" joint="right_knee_joint" gear="100" />
        <motor name="right_ankle_pitch_joint" joint="right_ankle_pitch_joint" gear="50" />
        <motor name="right_ankle_roll_joint" joint="right_ankle_roll_joint" gear="50" />

        <motor name="waist_yaw_joint" joint="waist_yaw_joint" gear="80" />
        <motor name="waist_roll_joint" joint="waist_roll_joint" gear="50" />
        <motor name="waist_pitch_joint" joint="waist_pitch_joint" gear="50" />

        <motor name="left_shoulder_pitch_joint" joint="left_shoulder_pitch_joint" gear="20" />
        <motor name="left_shoulder_roll_joint" joint="left_shoulder_roll_joint" gear="20" />
        <motor name="left_elbow_joint" joint="left_elbow_joint" gear="20" />
        <motor name="right_shoulder_pitch_joint" joint="right_shoulder_pitch_joint" gear="20" />
        <motor name="right_shoulder_roll_joint" joint="right_shoulder_roll_joint" gear="20" />
        <motor name="right_elbow_joint" joint="right_elbow_joint" gear="20" /> -->

    <position class="hip_motor" name="left_hip_pitch_joint" joint="left_hip_pitch_joint" />
    <position class="hip_motor" name="left_hip_roll_joint" joint="left_hip_roll_joint" />
    <position class="hip_motor" name="left_hip_yaw_joint" joint="left_hip_yaw_joint" />
    <position class="knee_motor" name="left_knee_joint" joint="left_knee_joint" />
    <position class="ankle_pitch_motor" name="left_ankle_pitch_joint" joint="left_ankle_pitch_joint" />
    <position class="ankle_roll_motor" name="left_ankle_roll_joint" joint="left_ankle_roll_joint" />

    <position class="hip_motor" name="right_hip_pitch_joint" joint="right_hip_pitch_joint" />
    <position class="hip_motor" name="right_hip_roll_joint" joint="right_hip_roll_joint" />
    <position class="hip_motor" name="right_hip_yaw_joint" joint="right_hip_yaw_joint" />
    <position class="knee_motor" name="right_knee_joint" joint="right_knee_joint" />
    <position class="ankle_pitch_motor" name="right_ankle_pitch_joint"
      joint="right_ankle_pitch_joint" />
    <position class="ankle_roll_motor" name="right_ankle_roll_joint" joint="right_ankle_roll_joint" />

    <position class="waist_motor" name="waist_yaw_joint" joint="waist_yaw_joint" />
    <position class="waist_motor" name="waist_roll_joint" joint="waist_roll_joint" />
    <position class="waist_motor" name="waist_pitch_joint" joint="waist_pitch_joint" />

    <position class="shoulder_pitch_motor" name="left_shoulder_pitch_joint"
      joint="left_shoulder_pitch_joint" />
    <position class="shoulder_roll_motor" name="left_shoulder_roll_joint"
      joint="left_shoulder_roll_joint" />
    <position class="elbow_motor" name="left_elbow_joint" joint="left_elbow_joint" />
    <position class="shoulder_pitch_motor" name="right_shoulder_pitch_joint"
      joint="right_shoulder_pitch_joint" />
    <position class="shoulder_roll_motor" name="right_shoulder_roll_joint"
      joint="right_shoulder_roll_joint" />
    <position class="elbow_motor" name="right_elbow_joint" joint="right_elbow_joint" />


    <!-- <motor name="left_shoulder_yaw_joint" joint="left_shoulder_yaw_joint" /> -->
    <!-- <motor name="left_wrist_roll_joint" joint="left_wrist_roll_joint" />
        <motor name="left_wrist_pitch_joint" joint="left_wrist_pitch_joint" />
        <motor name="left_wrist_yaw_joint" joint="left_wrist_yaw_joint" /> -->
    <!-- <motor name="right_shoulder_yaw_joint" joint="right_shoulder_yaw_joint" /> -->
    <!-- <motor name="right_wrist_roll_joint" joint="right_wrist_roll_joint" />
        <motor name="right_wrist_pitch_joint" joint="right_wrist_pitch_joint" />
        <motor name="right_wrist_yaw_joint" joint="right_wrist_yaw_joint" /> -->
  </actuator>

</mujoco>